<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.tdg-seville.info/benavides/featuremodelling/feature-model.xsd">
    <feature name="Car">
		<binaryRelation name="c1">
			<cardinality min="1" max="1"/>
			<solitaryFeature name="Body"/>
		</binaryRelation>
   		<binaryRelation name="c2">
			<cardinality min="1" max="1"/>
			<solitaryFeature name="Transmission">
				<setRelation name="c5">
					<cardinality min="1" max="1"/>
					<groupedFeature name="Automatic"/>
					<groupedFeature name="Manual"/>
				</setRelation>
			</solitaryFeature>
		</binaryRelation>
		<binaryRelation name="c3">
			<cardinality min="0" max="1"/>
			<solitaryFeature name="Pulls Trailer"/>
		</binaryRelation>
		<binaryRelation name="c4">
			<cardinality min="1" max="1"/>
			<solitaryFeature name="Engine">
				<setRelation name="c6">
					<cardinality min="1" max="2"/>
					<groupedFeature name="Gasoline"/>
					<groupedFeature name="Electric"/>
				</setRelation>
			</solitaryFeature>
		</binaryRelation>	
    </feature>
</feature-model>